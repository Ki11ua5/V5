@model ERP.Models.Almacen



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>
    <div class="card" style="width: 18rem; margin-top:2rem">
        <div class="card-body">
            
            <h6 class="card-subtitle mb-2 text-muted"></h6>
            <div>

                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.Altitud)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Altitud)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Latitud)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Latitud)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.codigo_postal)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.codigo_postal)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Estado1.Nom_Estado)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Estado1.Nom_Estado)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Ingeniero.Nombre)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Ingeniero.Nombre)
                    </dd>

                </dl>

                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color">
                        <input type="submit" value="Borrar" class="btn btn-danger" />
                        <a class="btn btn-primary" href="@Url.Action("Index","Almacens")"> Regresar</a>
                    </div>
                }
            </div>
            </div>
        </div>
</body>
</html>
