

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ImagenesS</title>
</head>
<body>
    <div>
        @foreach (var item in ViewBag.lista)
        {
            <img src="~/Clase/@item" width="300" height="300" alt="" />
            @Html.ActionLink("Borrar","BorrarImg",new { name=item })
        }
    </div>
</body>
</html>
